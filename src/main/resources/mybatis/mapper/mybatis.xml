<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="profile_test">
    <select id="Mainprofile"  resultType="com.inus.NY.profile.vo.profilelistVO">
        SELECT m_name, m_nickname, m_memo FROM MEMBERMANAGE_TBL
    </select>
    <select id="SelectMusicImport"  resultType="com.inus.NY.profile.vo.profilelistVO">
        SELECT * FROM recomandList
    </select>
    <insert id="ImgInsert"  parameterType="com.inus.NY.profile.vo.profilelistVO">
        insert into TEST_BLOB(T_IDX,T_BLOB)
        VALUES (#{idx},#{img})
    </insert>
    <select id="ImgSelect"  resultType="com.inus.NY.profile.vo.profilelistVO">
        SELECT T_BOLB
        FROM test_blob
        WHERE t_idx=#{idx}
    </select>
</mapper>